(set-logic QF_NRA)
(declare-fun xi () Real)
(declare-fun x0 () Real)
(declare-fun x1 () Real)
(declare-fun x2 () Real)
(declare-fun x3 () Real)
(declare-fun bi () Real)
(declare-fun b0 () Real)
(declare-fun b1 () Real)
(declare-fun b2 () Real)
(declare-fun b3 () Real)
(declare-fun s0_q_i () Real)
(declare-fun s0_q0 () Real)
(declare-fun s0_q1 () Real)
(declare-fun s0_q2 () Real)
(declare-fun s0_q3 () Real)
(declare-fun s1_q_i () Real)
(declare-fun s1_q0 () Real)
(declare-fun s1_q1 () Real)
(declare-fun s1_q2 () Real)
(declare-fun s1_q3 () Real)
(declare-fun t0 () Real)
(declare-fun t1 () Real)
(declare-fun t2 () Real)
(declare-fun t3 () Real)
(assert (= battery_charging_rate 50.0))
(assert (= battery_discharge_rate_fly 1.0))
(assert (= battery_discharge_rate_hover 1.0))
(assert (= drone_velocity 10.0))
(assert (= queue_data_rate 1.0))
(assert (= queue_upload_rate 1.0))
(assert (= s0_loc 10.0))
(assert (= s1_loc 15.0))
(assert (>= t0 0.0))
(assert (>= t1 0.0))
(assert (>= t2 0.0))
(assert (>= t3 0.0))
(assert (>= xi 0.0))
(assert (<= xi 10.0))
(assert (>= x0 0.0))
(assert (<= x0 10.0))
(assert (>= x1 0.0))
(assert (<= x1 10.0))
(assert (>= x2 0.0))
(assert (<= x2 10.0))
(assert (>= bi 0.0))
(assert (<= bi 100.0))
(assert (>= b0 0.0))
(assert (<= b0 100.0))
(assert (>= b1 0.0))
(assert (<= b1 100.0))
(assert (>= b2 0.0))
(assert (<= b2 100.0))
(assert (>= b3 0.0))
(assert (<= b3 100.0))
(assert (>= s0_q_i 0.0))
(assert (<= s0_q_i 100.0))
(assert (>= s0_q0 0.0))
(assert (<= s0_q0 100.0))
(assert (>= s0_q1 0.0))
(assert (<= s0_q1 100.0))
(assert (>= s0_q2 0.0))
(assert (<= s0_q2 100.0))
(assert (>= s0_q3 0.0))
(assert (<= s0_q3 100.0))
(assert (>= s1_q_i 0.0))
(assert (<= s1_q_i 100.0))
(assert (>= s1_q0 0.0))
(assert (<= s1_q0 100.0))
(assert (>= s1_q1 0.0))
(assert (<= s1_q1 100.0))
(assert (>= s1_q2 0.0))
(assert (<= s1_q2 100.0))
(assert (>= s1_q3 0.0))
(assert (<= s1_q3 100.0))
(declare-fun choice () Real)
(assert (or (= choice 0.0) (= choice 1.0)))


;charging
(assert (= xi 0))
(assert (= x0 (+ xi 0.0)))
(assert (= b0 (+ bi (* battery_charging_rate t3))))
(assert (= s0_q0 (+ s0_qi (* queue_data_rate t3))))
(assert (= s1_q0 (+ s1_qi (* queue_data_rate t3))))


;flying to sensors
(assert (=> (= choice 0.0) (= x1 s0.0_loc)))
(assert (=> (= choice 1.0) (= x1 s1.0_loc)))
(assert (= x1 (+ x0 (* drone_velocity t0))))
(assert (= b1 (+ b0 (* battery_discharge_rate_fly t0))))
(assert (= s0_q1 (+ s0_q0 (* queue_data_rate t0))))
(assert (= s1_q1 (+ s1_q0 (* queue_data_rate t0))))


;Collecting data
(assert (= x2 x1))
(assert (= b2 (+ b1 (* battery_charging_rate t2))))
(assert (=> (= choice 0.0) (and (= s0_q2 (- s0_q1 (* queue_upload_rate t2))) (+ s1_q1 (= s1_q2 (* queue_data_rate t2))))))
(assert (=> (= choice 1.0) (and (= s1_q2 (- s1_q1 (* queue_upload_rate t2))) (+ s0_q1 (= s0_q2 (* queue_data_rate t2))))))


;flying back
(assert (= x3 0))
(assert (= x3 (+ x2 (* (- (drone_velocity) t3)))))
(assert (= b3 (+ b2 (* battery_discharge_rate_fly t3))))
(assert (= s0_q3 (+ s0_q2 (* queue_data_rate t3))))
(assert (= s1_q3 (+ s1_q2 (* queue_data_rate t3))))


;Goal
(assert (not (=>(or (and (>= bi p0) (<= s1_qi p1) (<= s2_qi p2)) (and (>= bi p3) (<= s1_qi p4) (<= s2_qi p5)))(and (and (> b0 0) (> b1 0) (> b2 0) (> b3 0) (< s1_q0 100) (< s1_q1 100) (< s1_q2 100) (< s1_q3 100) (< s2_q0 100) (< s2_q1 100) (< s2_q2 100) (< s2_q3 100))(or (and (>= b3 p0) (<= s1_q3 p1) (<= s2_q3 p2)) (and (>= b3 p3) (<= s1_q3 p4) (<= s2_q3 p5)))))))
(check-sat)
(exit)
